{
  "display": "Package.js",
  "extends": "./base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
